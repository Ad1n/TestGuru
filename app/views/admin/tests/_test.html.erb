<tr>
  <td><%= test.title %></td>
  <td><%= test_level(test) %></td>
  <% if user_signed_in?  %>
    <td align="center"><%= test.questions.count %></td>
    <td><%= link_to t('.actions.show'), admin_test_path(test) %></td>
    <td><%= link_to t('.actions.edit'), edit_admin_test_path(test) %></td>
    <td><%= link_to t('.actions.delete'),
            admin_test_path(test),
            method: :delete,
            data: { confirm: I18n.t("confirm.message") } %>
    </td>
  <% end %>
</tr>